					<include name="Tao.Platform.Windows.dll" />
					<include name="SdlDotNet.dll" />
					<include name="Tao.FreeGlut.dll" />
					<include name="DevIL.NET.dll" />
					<include name="glselectwrap.dll" />
				</fileset>
			</copy>
